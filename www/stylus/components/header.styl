/* Header Styles */
.nav-bar {
  position: fixed;
  z-index: 100;
  top: 0;
  left: 0;
  right: 0;
  padding: 15px 0;
  height: 29px;
  font-size: 20px
  font-weight: 100;
  text-align: center;
  color: black;
  background: white;
  border-bottom: 1px solid #ececec;
}

// Title
.nav-bar__title {
  font-size: 24px
  font-weight: normal;
  padding: 3px 40px;
}
